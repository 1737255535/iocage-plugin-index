{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "openvpn"
  ],
  "properties": {
    "openvpn": {
      "$id": "#/properties/openvpn",
      "type": "object",
      "title": "The Plugin Name",
      "default": null,
      "required": [
        "MANIFEST",
        "name",
        "primary_pkg",
        "icon",
        "description",
        "official"
      ],
      "properties": {
        "MANIFEST": {
          "$id": "#/properties/openvpn/properties/MANIFEST",
          "type": "string",
          "title": "The Manifest Schema",
          "default": "",
          "examples": [
            "openvpn.json"
          ],
          "pattern": "^(.*)$"
        },
        "name": {
          "$id": "#/properties/openvpn/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "OpenVPN Server"
          ],
          "pattern": "^(.*)$"
        },
        "primary_pkg": {
          "$id": "#/properties/openvpn/properties/primary_pkg",
          "type": "string",
          "title": "The Primary_pkg Schema",
          "default": "",
          "examples": [
            "openvpn"
          ],
          "pattern": "^(.*)$"
        },
        "icon": {
          "$id": "#/properties/openvpn/properties/icon",
          "type": "string",
          "title": "The Icon Schema",
          "default": "",
          "examples": [
            "http://swupdate.openvpn.net/community/icons/ovpntech_key.png"
          ],
          "pattern": "^(.*)$"
        },
        "description": {
          "$id": "#/properties/openvpn/properties/description",
          "type": "string",
          "title": "The Description Schema",
          "default": "",
          "examples": [
            "Virtual private network software to create secure point-to-point connections."
          ],
          "pattern": "^(.*)$"
        },
        "official": {
          "$id": "#/properties/openvpn/properties/official",
          "type": "boolean",
          "title": "The Official Schema",
          "default": false,
          "examples": [
            false
          ]
        }
      }
    }
  }
}
